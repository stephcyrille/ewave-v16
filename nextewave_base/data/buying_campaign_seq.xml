<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="buying_campaign_sequence" model="ir.sequence">
      <field name="name">Buying campaign sequence</field>
      <field name="code">buying.campaign.sequence</field>
      <field name="prefix">CAMP/BUY/%(year)s/</field>
      <field name="padding">8</field>
      <field name="number_next">1</field>
    </record>

    <record id="buying_campaign_req_sequence" model="ir.sequence">
      <field name="name">Buying campaign request sequence</field>
      <field name="code">buying.campaign.request.sequence</field>
      <field name="prefix">REQ/BUY/%(year)s/</field>
      <field name="padding">8</field>
      <field name="number_next">1</field>
    </record>
  </data>
</odoo>