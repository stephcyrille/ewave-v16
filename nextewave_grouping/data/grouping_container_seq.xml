<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="grouping_container_sequence" model="ir.sequence">
      <field name="name">Grouping container sequence</field>
      <field name="code">grouping.container.sequence</field>
      <field name="prefix">CONTAINER/%(year)s/</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
    </record>

    <record id="grouping_container_tracking_sequence" model="ir.sequence">
      <field name="name">Grouping container tracking sequence</field>
      <field name="code">grouping.container.tracking.sequence</field>
      <field name="prefix">N-%(year)s</field>
      <field name="padding">4</field>
      <field name="suffix">%(month)s</field>
      <field name="number_next">1</field>
    </record>
  </data>
</odoo>